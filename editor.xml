<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="FONT_SCALE" value="1.25" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="DejaVu Sans Mono" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="important" pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\bnote\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\bbacklog\b.*">
      <option name="FOREGROUND" value="a83965" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>